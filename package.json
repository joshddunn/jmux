{
  "name": "@joshddunn/jmux",
  "version": "0.1.1",
  "description": "Automate building new tmux sessions.",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "start": "node lib/index.js start -f ./jmux.yaml",
    "stop": "node lib/index.js stop -f ./jmux.yaml",
    "jmux": "node lib/index.js",
    "local": "sudo npm i -g && jmux"
  },
  "author": "Josh Dunn",
  "license": "ISC",
  "bin": {
    "jmux": "./lib/index.js"
  },
  "dependencies": {
    "@jcoreio/kexec": "^4.0.0",
    "commander": "^9.4.0",
    "yaml": "^2.1.1"
  }
}
