  "license": "ISC",
  "dependencies": {
    "commander": "^9.4.0",
    "joi": "^17.13.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
