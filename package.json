{
  "name": "@joshddunn/jmux",
  "repository": "github:joshddunn/jmux",
  "version": "0.7.0",
  "description": "Automate building new tmux sessions.",
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=18.0.0 <19.0.0"
  },
  "main": "./dist/index.js",
  "bin": {
    "jmux": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "typecheck": "npm run build --noEmit",
    "jmux": "npm run build && node dist/index.js",
    "test": "jest",
    "local": "sudo npm i -g && jmux"
  },
